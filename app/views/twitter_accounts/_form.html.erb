<div id="search-form-wrapper" class="card-panel z-depth-1">
<div class="container">
	<div class="row">
	<%= form_tag '/twitter_accounts', :method => 'post', :id => "search" do %>
		<%= fields_for @twitter_account do |account_form| %>
	    <div id="screenname-prefix" class="col s1">@</div><div class="col s11"><%= account_form.text_field :screenname %></div>
	  <% end %>
  <% end %>
  </div>
</div>
</div>